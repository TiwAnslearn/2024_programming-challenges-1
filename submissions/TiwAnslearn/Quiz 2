a_z = "abcdefghijklmnopqrstuvwxyz"
string = input("Enter string:")
#'the quick brown fox jumps over the lazy dog'

Testif = True
for i in a_z:
    if i not in string:
        Testif = False
        break
if Testif:
    y =string.split()
    y.sort(reverse=True, key = len)
    print(y[0])

else:
    print("Not a Pangram")
